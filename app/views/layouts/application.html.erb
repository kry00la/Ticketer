<!DOCTYPE html>
<html>
<head>
  <title>Ticketer</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	
<% flash.each do |key, value| %>
<div class='flash' id='<%= key %>'>
<p><%= value %></p>
</div>
<% end %>

<%= yield %>

</body>
</html>
